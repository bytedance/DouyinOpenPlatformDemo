<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/jump_douyin_user_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转个人主页"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/jump_douyin_user_contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转会话页"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/jump_douyin_user_profile" />

    <Button
        android:id="@+id/jump_production"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转名片作品管理页"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/jump_douyin_user_contact" />

</androidx.constraintlayout.widget.ConstraintLayout>